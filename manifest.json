{
  "manifest_version": 2,

  "name": "TimeWatch",
  "description": "This extension will track time spent on designated website",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

   "background": {
    "scripts":["js/background.js",
              "js/jquery-2.1.4.min.js"
    ]

  },

  "permissions": [
    "activeTab"
  ]

 
}
